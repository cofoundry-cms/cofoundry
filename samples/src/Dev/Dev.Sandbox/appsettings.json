{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "SandboxSite": {
    "ContactRequestNotificationToAddress": "test@example.com"
  },

  "Cofoundry": {

    "Admin": {
      "DirectoryName": "sandbox-admin"
    },

    "Database": {
      "ConnectionString": "Server=.\\sqlexpress;Database=Cofoundry.Dev.Sandbox;Integrated Security=True;Encrypt=False;MultipleActiveResultSets=True"
    },

    "Mail": {
      "DefaultFromAddress": "no-reply@example.com"
    },

    "Debug": {
      "DeveloperExceptionPageMode": "DevelopmentOnly"
    },

    "StaticFiles": {
      "MaxAge": 12345,
      "CacheMode": "OnlyVersionedFiles"
    }
  }
}
