{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "BasicTestSite": {
    "ContactRequestNotificationToAddress": "ignore-me@simpletestsample.org"
  },

  "Cofoundry": {

    "Database": {
      "ConnectionString": "Server=.\\sqlexpress;Database=Cofoundry.BasicTestSite;Integrated Security=True;MultipleActiveResultSets=True"
    },

    "Mail": {
      "DefaultFromAddress": "test@cofoundry.com"
    },

    "Debug": {
      "DeveloperExceptionPageMode": "DevelopmentOnly"
    },

    "Pages": {
      "xDisabled": true
    },

    "ImageAssets": {
      "xDisabled": false,
      "xEnableCompatibilityRoutesFor0_4": "true",
      "xDisableResizing": false,
      "xMaxUploadWidth": 500,
      "xMaxUploadHeight": 500,
      "xMaxResizeWidth": 1200,
      "xMaxResizeHeight": 1200,
      "xCacheMaxAge": 60
    },

    "DocumentAssets": {
      "xDisabled": false,
      "xEnableCompatibilityRoutesFor0_4": "false",
      "xCacheMaxAge": 60
    },

    "AssetFiles": {
      "xFileExtensionValidation": "UseWhitelist",
      "xMimeTypeValidation": "UseBlacklist",
      "xFileExtensionValidationList": [
        "csv"
      ],
      "xMimeTypeValidationList": [ "image/png", "test other" ]
    },

    "SiteUrlResolver": {
      "SiteUrlRoot": "http://localhost:61153/"
    },

    "StaticFiles": {
      "MaxAge": 12345,
      "CacheMode": "OnlyVersionedFiles"
    },

    "Plugins": {

      "AzureBlobFileService": {
        "ConnectionString": "TODO: Add Conntection String"
      },

      "Hangfire": {
        "Disabled": false,
        "EnableHangfireDashboard": true
      }
    }
  }
}
